import java.util.*;
public class Solution {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
        String str=sc.nextLine();
        String vowels="aeiou";
        int count=0;
        for(int i=0;i<str.length();i++){
     if(vowels.indexOf(str.charAt(i))!=-1){
    count++;
}}
        System.out.print(count);
}
}
