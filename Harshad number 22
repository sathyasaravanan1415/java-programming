import java.util.*;
public class main{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int temp=a;
        int sum=0;
        while(temp>0){
            int rem=temp%10;
            sum+=rem;
            temp/=10;
        }
        if(a%sum==0)
        {
            System.out.println("Harshad Number");
        }else{
            System.out.println("Not Harshad Number");
        }
            
            
        }
        
        
   }
